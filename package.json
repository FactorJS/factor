{
  "name": "factor",
  "version": "3.2.7",
  "scripts": {
    "deploy": "yarn release && yarn push-release",
    "push-release": "git checkout release && git rebase main && git push --no-verify && git checkout main",
    "test-fixture": "TEST_FIXTURES=1 jest",
    "dash": "yarn factor boot --workspace @darwin_/dashboard",
    "site": "yarn factor boot --workspace @darwin_/site",
    "framework": "yarn factor boot --workspace @factor/site",
    "fiction": "yarn factor boot --workspace @fiction/site",
    "dsite-build": "yarn workspace @darwin_/site factor prerender",
    "dlink": "yarn link @darwin_/query @darwin_/db @darwin_/utils @darwin_/types",
    "release": "npx factor release",
    "pack": "yarn workspace @factor/build factor pack",
    "dev": "yarn workspace @factor/site factor dev",
    "test": "npm run test-lint && npm run test-unit",
    "test-lint": "npm run test-eslint && npm run test-types",
    "test-eslint": "eslint --ext .ts,.js,.vue,.json .",
    "test-types": "tsc --skipLibCheck --excludeDirectories node_modules",
    "test-unit": "npx jest --forceExit --passWithNoTests",
    "test-integration": "npx jest test/integration --forceExit --passWithNoTests",
    "format-all": "yarn prettier --ext .ts,.js,.vue,.json . --write",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "up": "docker-compose up",
    "up-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up"
  },
  "license": "MIT",
  "workspaces": [
    "@core/*",
    "@apps/*",
    "@plugins/*",
    "@test/*",
    "cdk"
  ],
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "gitHooks": {
    "pre-push": "git pull && npm run test-lint",
    "commit-msg": "ts-node scripts/verify-commit.ts"
  },
  "devDependencies": {
    "@jest/types": "^27.0.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "chalk": "^4.1.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "eslint-plugin-vue": "^7.1.0",
    "jest": "^27.0.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "playwright-chromium": "^1.10.0",
    "prettier": "^2.0.4",
    "prettier-eslint": "^13.0.0",
    "sirv": "^1.0.11",
    "tailwindcss-cli": "^0.1.2",
    "ts-jest": "^27.0.0",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@darwin_/blog": "^3.1.8",
    "autoprefixer": "^10.2.4",
    "eslint-plugin-implicit-dependencies": "^1.1.0",
    "postcss": "^8.2.6",
    "tailwindcss": "^2.1.0",
    "typescript": "^4.0.0"
  }
}
