{
  "name": "factor",
  "version": "4.1.41",
  "main": "index.ts",
  "publishConfig": {},
  "type": "module",
  "scripts": {
    "prerender": "npm exec -w @factor/site -- factor prerender",
    "deploy": "npm release && npm push-release",
    "site": "npm -w @factor/site exec -- factor dev",
    "release": "npm exec -- factor release",
    "pack": "npm -w @factor/build exec -- factor pack",
    "dev": "npm -w @factor/site exec -- factor dev",
    "test": "npm run test-lint && npm run test-unit",
    "test-pkg": "npx npmPkgJsonLint .",
    "test-unit": "npm exec -- vitest run",
    "test-lint": "npm run test-pkg && npm run test-types && npm run test-eslint && npm run test-vue",
    "test-vue": "eslint --ext .vue @core/. @apps/. @plugins/.",
    "test-eslint": "LINT_ENV=basic eslint --ext .ts,.js @core/. @apps/. @plugins/.",
    "test-types": "tsc --skipLibCheck --excludeDirectories node_modules",
    "format-all": "npm prettier --ext .ts,.js,.vue,.json . --write",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "up": "docker-compose up",
    "up-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up"
  },
  "license": "MIT",
  "workspaces": [
    "@apps/*",
    "@plugins/*",
    "@core/*",
    "@engine/*",
    "@test/*",
    "cdk"
  ],
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "gitHooks": {
    "pre-push": "git pull && npm run test",
    "commit-msg": "ts-node scripts/verify-commit.mjs"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.16",
    "chalk": "^5.0.0",
    "happy-dom": "^2.31.1",
    "jsdom": "^19.0.0",
    "nodemon": "^2.0.15",
    "npm-package-json-lint": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "sirv": "^2.0.2",
    "tailwindcss-cli": "^0.1.2",
    "tslib": "^2.3.1",
    "vite": "^2.7.13",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@factor/api": "4.1.41",
    "@factor/cli": "4.1.41",
    "@factor/engine": "4.1.41",
    "@factor/render": "4.1.41",
    "@factor/site": "4.1.41",
    "@factor/test": "4.1.41",
    "@vitejs/plugin-vue": "^2.2.2",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.0",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.5"
  }
}
