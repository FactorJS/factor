{
  "name": "factor",
  "version": "3.4.10",
  "scripts": {
    "deploy": "yarn release && yarn push-release",
    "push-release": "git checkout release && git rebase main && git push --no-verify && git checkout main",
    "test-fixture": "TEST_FIXTURES=1 jest",
    "site": "yarn factor dev -w @factor/site",
    "fiction": "yarn factor dev -w @fiction/site",
    "release": "npx factor release",
    "pack": "yarn workspace @factor/build factor pack",
    "dev": "yarn workspace @factor/site factor dev",
    "test": "npm run test-lint && npm run test-unit",
    "test-lint": "npm run test-eslint && npm run test-types",
    "test-eslint": "LINT_ENV=basic eslint --ext .ts,.js @core/. @apps/. @plugins/.",
    "test-types": "tsc --skipLibCheck --excludeDirectories node_modules",
    "test-unit": "npx jest --forceExit --passWithNoTests",
    "test-integration": "npx jest test/integration --forceExit --passWithNoTests",
    "format-all": "yarn prettier --ext .ts,.js,.vue,.json . --write",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "up": "docker-compose up",
    "up-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up"
  },
  "license": "MIT",
  "workspaces": [
    "@core/*",
    "@apps/*",
    "@plugins/*",
    "@test/*",
    "cdk"
  ],
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "gitHooks": {
    "pre-push": "git pull && npm run test-lint",
    "commit-msg": "ts-node scripts/verify-commit.mjs"
  },
  "devDependencies": {
    "@jest/types": "^27.2.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "chalk": "^5.0.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "latest",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-json": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-simple-import-sort": "latest",
    "eslint-plugin-unicorn": "^39.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "playwright-chromium": "^1.16.2",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "sirv": "^1.0.18",
    "tailwindcss-cli": "^0.1.2",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@kaption/blog": "^3.1.12",
    "autoprefixer": "^10.3.7",
    "postcss": "^8.3.11",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.4.4"
  }
}
